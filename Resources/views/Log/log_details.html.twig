{% extends 'ClarolineCoreBundle:Log:view_details.html.twig' %}

{% block logDetailsContext %}
    {{ parent() }}
    {% if constant('Icap\\PortfolioBundle\\Event\\Log\\PortfolioAddEvaluatorEvent::ACTION') == log.action %}
        {% include 'IcapPortfolioBundle:Log:log_details_portfolio_add_evaluator.html.twig' %}
    {% elseif constant('Icap\\PortfolioBundle\\Event\\Log\\PortfolioRemoveEvaluatorEvent::ACTION') == log.action %}
        {% include 'IcapPortfolioBundle:Log:log_details_portfolio_remove_evaluator.html.twig' %}
    {% endif %}
{% endblock %}